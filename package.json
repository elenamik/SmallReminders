  },
  "homepage": "https://github.com/elenamik/SmallReminders#readme",
  "dependencies": {
    "@types/react": "^16.9.17",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-config-standard": "^14.1.0",
    "express": "^4.17.1",
    "firebase": "^7.6.2",
    "firebase-functions": "^3.3.0",
    "mongodb": "^3.4.0",
